    <style>
        .instrument {
            width: 100%;
            height: unset;
            position: relative;
            box-sizing: border-box;
            padding: 0 13%;
        }
        .drum-name, .key-hint, .midi-hint {
            font: 9pt sans-serif;
            fill: #fffc;
            text-anchor: middle;
            pointer-events: none;
            dominant-baseline: central;
            user-select: none;
            -moz-user-select: none;
            -webkit-user-select: none;
        }
        .midi-hint, .key-hint { opacity: 0; }
        .key-hint { font: 11pt sans-serif; fill: #fff0; }
        .key-text { font: 27pt sans-serif; fill: #fff8; }
        .show { opacity: 1.0; }
        .credit {
            color: #888;
            font-size: 11px;
            user-select: none;
            font-family: Arial, Helvetica, sans-serif; 
        }
        .credit a { color: #555; }
        .credit a:hover { color: cyan; }
        .credit a:active { color: red; }
        .drum-hint {
            position: absolute;
            width: 40vw;
            height: 5vh;
            left: 30vw;
            padding: 10px 15px;
            border-radius: 5px;
            user-select: none;
            pointer-events: none;
            text-align: center;
            font-family: 'Courier New', Courier, monospace;
            font-weight: bold;
            line-height: 120%;
            font-size: 13px;
        }
    </style>
